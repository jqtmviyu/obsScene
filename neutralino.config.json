{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.neutralino.obsScene",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "enableExtensions": true,
  "exportAuthInfo": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "OBS",
      "width": 124,
      "height": 86,
      "minWidth": 124,
      "minHeight": 86,
      "x": 1792,
      "y": 405,
      "useSavedState": false,
      "center": false,
      "alwaysOnTop": true,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "resizable": false,
      "maximize": false,
      "borderless": true,
      "hidden": false,
      "transparent": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "obsScene",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.0.0",
    "clientVersion": "5.0.1"
  },
  "buildScript": {
    "mac": {
      "architecture": [
        "x64",
        "arm64",
        "universal"
      ],
      "minimumOS": "10.13.0",
      "appName": "obsScene",
      "appBundleName": "obsScene",
      "appIdentifier": "js.neutralino.obsScene",
      "appIcon": "icon.icns"
    },
    "win": {
      "architecture": [
        "x64"
      ],
      "appName": "obsScene",
      "appIcon": "icon.ico"
    },
    "linux": {
      "architecture": [
        "x64",
        "arm64",
        "armhf"
      ],
      "appName": "obsScene",
      "appIcon": "icon.png",
      "appPath": "/usr/share/obsScene",
      "appIconPath": "/usr/share/obsScene/icon.png"
    }
  }
}